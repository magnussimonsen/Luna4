{
  "English": [
    {
      "label": "solve",
      "type": "keyword",
      "detail": "Solve equation(s) for variable(s)",
      "info": "Example:\n solve 2x + 1 = 0 for x\n solve x + y = 1 and x - 3y = 5 for x and y"
    },
    {
      "label": "csolve",
      "type": "keyword",
      "detail": "Allows complex solutions",
      "info": "Example:\n csolve x^2 + 1 = 0 for x"
    },
    {
      "label": "eval",
      "type": "keyword",
      "detail": "Evaluate an expression",
      "info": "Example:\n eval(1/3) = 0.333333...\n eval(1/3, 2) = 0.33"
    },
    {
      "label": "expand",
      "type": "keyword",
      "detail": "Expand an expression",
      "info": "Example:\n expand((x + 1)^2) = x^2 + 2x + 1"
    },
    {
      "label": "factor",
      "type": "keyword",
      "detail": "Factor an expression",
      "info": "Example:\n factor(x^2 -4) = (x - 2)(x + 2)"
    },
    {
      "label": "simp",
      "type": "keyword",
      "detail": "Simplify an expression",
      "info": "Example:\n simp(x^2 + 2x + 1)"
    },
    {
      "label": "series",
      "type": "keyword",
      "detail": "Compute the series expansion of expression",
      "info": "Example:\n series(x^2 + 3x + 4, x, 0, 5)\n series(x^2 + 3x + 4, x, 0, 5).removeO()\n series(x^2 + 3x + 4, x, 0, 5).removeO(1)"
    },
    {
      "label": "diff",
      "type": "keyword",
      "detail": "Differentiate an expression",
      "info": "First order derivative:\n diff(x^2+3x+4,x)\n Second order derivative: diff(x^2+3x+4,x,2)"
    },
    {
      "label": "integral",
      "type": "keyword",
      "detail": "Integrate an expression",
      "info": "Examples:\n integral(x^2+3x+4,x) \n integral(x^2+3x+4, x, 0, 1)"
    },
    {
      "label": "limit",
      "type": "keyword",
      "detail": "Compute the limit of an expression",
      "info": "Example:\n limit(x^2 + 3x + 4, x, oo)\n limit(x^2 + 3x + 4, x, oo).subs(x, oo)"
    },
    {
      "label": "Mat",
      "type": "keyword",
      "detail": "Create a matrix",
      "info": "Example:\n Matrix([[1, 2], [3, 4]])\nMatrix([[1, 2], [3, 4]]).det() \nMatrix([[1, 2], [3, 4]]).inv()"
    },
    {
      "label": "det",
      "type": "keyword",
      "detail": "Calculate the determinant",
      "info": "Example:\n det([[1, 2], [3, 4]]) = -2\n det([[1, 2], [3, 4]]).subs(x, 0)"
    },
    {
      "label": "inv",
      "type": "keyword",
      "detail": "Compute the inverse of a matrix",
      "info": "Example:\n inv([[1, 2], [3, 4]]) = [[-2, 1], [1.5, -0.5]]"
    },
    {
      "label": "transpose",
      "type": "keyword",
      "detail": "Transpose a matrix",
      "info": "Example:\n transpose([[1, 2], [3, 4]]) = [[1, 3], [2, 4]]"
